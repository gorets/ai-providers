{
  "name": "@gorets/ai-providers",
  "version": "1.0.10",
  "description": "Comprehensive database of AI model providers, models, pricing, and capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "data",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:test": "tsc -p tsconfig.test.json",
    "generate": "npm run build && node scripts/build-json.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run generate",
    "test": "npm run build:test && node --test dist/tests/**/*.test.js",
    "version": "npm run generate && git add data/"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "claude",
    "gpt",
    "pricing",
    "models",
    "providers",
    "machine-learning",
    "artificial-intelligence"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorets/ai-providers.git"
  },
  "author": "Gorets",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
